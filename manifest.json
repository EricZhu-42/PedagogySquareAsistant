{
    "name": "教学立方辅助工具",
    "version": "2.1.1",
    "description": "适用于教学立方（PedagogySquare）的文件下载、作业批改辅助增强工具",
    "homepage_url":"https://ladderoperator.top/2020/04/02/pedagogysquareasistant/",
    "icons": {
      "256":"logo.png",
      "48":"logo_48x48.png"
    },

    "background": {
      "persistent": false,
      "scripts" : ["background.js"],
      "all_frames":false
    },

    "browser_action": {
      "default_icon": {
        "256":"logo.png",
        "48":"logo_48x48.png"
      }
    },
    "web_accessible_resources": [
      "catchHomework.js"
    ],
    "options_ui":{
      "page": "options.html",
      "chrome_style": true
    },
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus",
      "tabs",
      "https://teaching.applysquare.com/*"
    ],
    "manifest_version": 2

  }