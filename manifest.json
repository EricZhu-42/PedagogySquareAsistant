{
    "name": "教学立方辅助工具",
    "version": "2.0.1",
    "description": "天下苦教学立方久矣！做此插件以方便无论老师还是同学！",
    "homepage_url":"https://ladderoperator.top/2020/04/02/teachingsquareassistant/",
    "icons":{
      "32":"logo.png"
    },

    "background": {
      "persistent": false,
      "scripts" : ["background.js"],
      "all_frames":false
    },

    "browser_action": {
      "default_icon": "logo.png",
      "icons":"logo.png"
    },
    "web_accessible_resources": [
      "catchHomework.js"
    ],
    "options_ui":{
      "page": "options.html",
      "chrome_style": true
    },
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus",
      "tabs",
      "<all_urls>"
    ],
    "manifest_version": 2

  }